{
  "name": "Bookmark note",
  "version": "1.0",
  "description": "Bookmark note!",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "css": ["style.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Your bookmark note."
  },
  "manifest_version": 2
}
