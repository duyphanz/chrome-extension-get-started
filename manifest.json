{
  "manifest_version": 2,
  "name": "Bookmark note",
  "version": "1.0",
  "description": "Bookmark note!",
  "permissions": ["bookmarks", "storage", "tabs"],
  "icons": {
    "16": "images/icon.png", 
    "48": "images/icon.png", 
    "128": "images/icon.png"
},
  "browser_action": {
    "default_title": "Your bookmark note.",
    "default_popup": "popup.html"
  }
}
